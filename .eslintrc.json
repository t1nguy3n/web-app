{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "prettier"
    ],
    "rules": {
        "emitWarning": true,
        "linebreak-style": "off",
        "global-require": 0,
        "no-return-assign": 0,
        "react/destructuring-assignment": 0,
        "comma-dangle": [
            "error",
            "never"
        ],
        "import/prefer-default-export": 0,
        "import/no-dynamic-require": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "jsx-a11y/click-events-have-key-events": 0,
        "react/prop-types": 0,
        "no-shadow": 0,
        "no-param-reassign": 0,
        "no-use-before-define": 0,
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "consistent-return": 0,
        "prefer-destructuring": 0,
        "react/prefer-stateless-function": 0,
        "react/no-unused-state": 0,
        "no-unused-expressions": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/jsx-indent": [
            true,
            2
        ],
        "prefer-const": 0,
        "indent": 0,
        "no-tabs": 0,
        "arrow-parens": [
            "error",
            "always"
        ],
        "arrow-body-style": [
            "error",
            "as-needed",
            {
                "requireReturnForObjectLiteral": true
            }
        ],
        "no-unused-vars": [
            "warn",
            {
                "args": "after-used"
            }
        ],
        "jsx-a11y/anchor-is-valid": [
            "error",
            {
                "components": [
                    "Link"
                ],
                "specialLink": [
                    "to"
                ],
                "aspects": [
                    "noHref",
                    "invalidHref",
                    "preferButton"
                ]
            }
        ]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    }
}